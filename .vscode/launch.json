  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Debug",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug"
    },
    {
      "name": "Flutter Debug Web No null safety",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--web-renderer html","--no-sound-null-safety"],

    },
    {
      "name": "Flutter Debug No null Safety",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--no-sound-null-safety"]
    },
    {
      "name": "Flutter Profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "Flutter Release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
